<p id="notice"><%= notice %></p>

<h1>Arts</h1>

<table>
  <thead>
    <tr>
      <th>Auteur</th>
      <th>Titre</th>
      <th>Date</th>
      <th>Dimensions</th>
      <th>Photo</th>
      <th>Emotion</th>
      <th>Citation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @arts.each do |art| %>
      <tr>
        <td><%= art.auteur %></td>
        <td><%= art.titre %></td>
        <td><%= art.date %></td>
        <td><%= art.dimensions %></td>
        <td><img width="300px" src="<%= art.photo %>"></td>
        <td><%= art.emotion %></td>
        <td><%= art.citation %></td>
        <td><%= link_to 'Show', art %></td>
        <td><%= link_to 'Edit', edit_art_path(art) %></td>
        <td><%= link_to 'Destroy', art, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Art', new_art_path %>
